    "vite": "^5.0.3",
    "vite-plugin-tailwind-purgecss": "0.2.0"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "1.6.1",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.5",
