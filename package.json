    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "8.56.0",
    "@types/express": "^4.17.21",
    "@types/knex": "^0.16.1",
    "@types/node": "20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
